@using CvMakerApp.Entity.Entities
@model List<Membership>
@{
    ViewData["Title"] = "Memberships";
    Layout = "~/Views/Admin/Index.cshtml";
    var count = 0;
}

<h1>Üyelik ve Organizasyonlar</h1>
<table class="table-striped table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>organizasyon</th>
            <th>Pozisyon</th>
            <th>Başlangıç Tarihi</th>
            <th>Bitiş Tarihi</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(++count)</td>
                <td>@item.Organisation</td>
                <td>@item.Position</td>
                <td>@item.StartDate</td>
                <td>@item.EndDate</td>
                <td><a href="~/Default/DeleteMembership/@item.MembershipId" class="btn btn-danger">Sil</a></td>
                <td><a href="~/Default/UpdateMembership/@item.MembershipId" class="btn btn-warning">Güncelle</a></td>
            </tr>
        }
    </tbody>
</table>
<td><a href="~/Default/AddMembership/" class="btn btn-primary">Ekle</a></td>

