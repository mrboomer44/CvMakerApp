


@using CvMakerApp.Entity.Entities
@model List<Stack>
@{
    ViewData["Title"] = "Stack";
    Layout = "~/Views/Admin/Index.cshtml";
    var count = 0;
}

<h1>Teknik Beceriler</h1>
<table class="table-striped table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Beceriler</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(++count)</td>
                <td>@item.Description</td>
                <td><a href="~/Default/DeleteStack/@item.StackId" class="btn btn-danger">Sil</a></td>
                <td><a href="~/Default/UpdateStack/@item.StackId" class="btn btn-warning">Güncelle</a></td>
            </tr>
        }
    </tbody>
</table>
<td><a href="~/Default/AddStack/" class="btn btn-primary">Ekle</a></td>



